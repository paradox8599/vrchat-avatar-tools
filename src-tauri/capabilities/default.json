{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "remote": {
    "urls": [
      "https://vrchat.com",
      "https://vrchat-avatar-tools.paradox8599.io"
    ]
  },
  "permissions": [
    "core:default",
    "store:default",
    "shell:default",
    "clipboard-manager:allow-write-text",
    "notification:default",
    "autostart:default",
    "updater:default",
    "process:default",
    "cli:default",
    "aptabase:allow-track-event"
  ]
}
