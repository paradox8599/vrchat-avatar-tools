{
  "name": "vrchat_avatar_tools",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "postinstall": "git config core.hooksPath .git-hooks",
    "dev:next": "next dev",
    "debug:next": "NODE_OPTIONS='--inspect' next dev",
    "build:next": "next build",
    "start:next": "next start",
    "lint": "next lint",
    "tauri:build": "tauri build",
    "dev": "pwsh scripts/dev.ps1",
    "ver": "bun run scripts/ver.ts",
    "build": "bun run lint && pwsh scripts/build.ps1",
    "b": "bun run build",
    "_root": "git rev-parse --show-toplevel",
    "env": "bun run scripts/env.ts",
    "push": "bun run scripts/push.ts",
    "deploy": "bun run build && bun run push",
    "release": "bun run lint && bun run ver patch && bun run deploy"
  },
  "dependencies": {
    "@aptabase/tauri": "https://github.com/aptabase/tauri-plugin-aptabase#v2",
    "@radix-ui/react-avatar": "^1.1.1",
