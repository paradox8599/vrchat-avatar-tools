{
  "name": "vrchat_avatar_tools",
  "version": "0.1.32",
  "private": true,
  "scripts": {
    "postinstall": "git config core.hooksPath .git-hooks",
    "dev:next": "next dev",
    "build:next": "next build",
    "start:next": "next start",
    "lint": "next lint",
    "tauri:build": "tauri build",
    "dev": "cd $(git rev-parse --show-toplevel) && pwsh scripts/dev.ps1",
    "ver": "cd $(git rev-parse --show-toplevel) && bun run scripts/ver.ts",
    "build": "cd $(git rev-parse --show-toplevel) && bun run lint && pwsh scripts/build.ps1",
    "b": "bun run build",
    "_root": "git rev-parse --show-toplevel",
    "env": "cd $(git rev-parse --show-toplevel) && bun run scripts/env.ts",
    "push": "cd $(git rev-parse --show-toplevel) && bun run scripts/push.ts",
    "deploy": "cd $(git rev-parse --show-toplevel) && bun run build && bun run push",
    "release": "cd $(git rev-parse --show-toplevel) && bun run lint && bun run ver patch && bun run deploy"
  },
  "dependencies": {
    "@aptabase/tauri": "https://github.com/aptabase/tauri-plugin-aptabase#v2",
    "@radix-ui/react-avatar": "^1.1.1",
    "@radix-ui/react-checkbox": "^1.1.2",
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-dropdown-menu": "^2.1.2",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-popover": "^1.1.2",
    "@radix-ui/react-progress": "^1.1.0",
    "@radix-ui/react-scroll-area": "^1.2.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-switch": "^1.1.1",
    "@radix-ui/react-toast": "^1.2.2",
    "@radix-ui/react-tooltip": "^1.1.3",
    "@tauri-apps/plugin-autostart": "^2.0.0",
    "@tauri-apps/plugin-cli": "^2.0.0",
    "@tauri-apps/plugin-clipboard-manager": "^2.0.0",
    "@tauri-apps/plugin-notification": "^2.0.0",
    "@tauri-apps/plugin-process": "^2.0.0",
    "@tauri-apps/plugin-shell": "^2.0.0",
    "@tauri-apps/plugin-store": "^2.0.0",
    "@tauri-apps/plugin-updater": "^2.0.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cmdk": "1.0.0",
    "date-fns": "^3.6.0",
    "input-otp": "^1.2.4",
    "lodash": "^4.17.21",
    "lucide-react": "^0.439.0",
    "next": "14.2.8",
    "next-themes": "^0.3.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "swr": "^2.2.5",
    "tailwind-merge": "^2.5.3",
    "tailwindcss-animate": "^1.0.7",
    "valtio": "^2.0.0"
  },
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.665.0",
    "@tauri-apps/cli": "^2.0.1",
    "@types/bun": "^1.1.10",
    "@types/lodash": "^4.17.10",
    "@types/node": "^20.16.10",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.0",
    "eslint": "^8.57.1",
    "eslint-config-next": "14.2.8",
    "eslint-plugin-valtio": "^0.6.4",
    "lint-staged": "^15.2.10",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "semver": "^7.6.3",
    "tailwindcss": "^3.4.13",
    "typescript": "^5.6.2"
  },
  "lint-staged": {
    "*.{tsx,jsx,ts,js}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
